<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FAFAF5"
    tools:context=".Japanese">


    <ImageButton
        android:id="@+id/recipe1"
        android:layout_width="175dp"
        android:layout_height="135dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="385dp"
        android:scaleType="fitCenter"
        android:src="@drawable/food_syougayaki"
        app:layout_constraintBottom_toTopOf="@+id/toMain1"
        app:layout_constraintEnd_toStartOf="@+id/recipe2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/category1"
        tools:srcCompat="@tools:sample/backgrounds/scenic" />


    <ImageButton
        android:id="@+id/recipe2"
        android:layout_width="175dp"
        android:layout_height="135dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="385dp"
        android:scaleType="fitCenter"
        android:src="@drawable/food_syougayaki"
        app:layout_constraintBottom_toTopOf="@+id/toMain1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/recipe1"
        app:layout_constraintTop_toBottomOf="@+id/category3"
        tools:srcCompat="@tools:sample/backgrounds/scenic" />

    <ImageButton
        android:id="@+id/recipe3"
        android:layout_width="175dp"
        android:layout_height="135dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="400dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="385dp"
        android:scaleType="fitCenter"
        android:src="@drawable/food_syougayaki"
        app:layout_constraintBottom_toTopOf="@+id/toMain1"
        app:layout_constraintEnd_toStartOf="@+id/recipe4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/category1"
        app:layout_constraintVertical_bias="0.498"
        tools:srcCompat="@tools:sample/backgrounds/scenic" />

    <ImageButton
        android:id="@+id/recipe4"
        android:layout_width="175dp"
        android:layout_height="135dp"
        android:layout_marginTop="400dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:layout_marginBottom="385dp"
        android:scaleType="fitCenter"
        android:src="@drawable/food_syougayaki"
        app:layout_constraintBottom_toTopOf="@+id/toMain1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/recipe3"
        app:layout_constraintTop_toBottomOf="@+id/category3"
        tools:srcCompat="@tools:sample/backgrounds/scenic" />


    <ImageButton
        android:id="@+id/recipe5"
        android:layout_width="175dp"
        android:layout_height="135dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="750dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="385dp"
        android:scaleType="fitCenter"
        android:src="@drawable/food_syougayaki"
        app:layout_constraintBottom_toTopOf="@+id/toMain1"
        app:layout_constraintEnd_toStartOf="@+id/recipe6"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/category1"
        app:layout_constraintVertical_bias="0.497"
        tools:srcCompat="@tools:sample/backgrounds/scenic" />

    <ImageButton
        android:id="@+id/recipe6"
        android:layout_width="175dp"
        android:layout_height="135dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:layout_marginBottom="124dp"
        android:scaleType="fitCenter"
        android:src="@drawable/food_syougayaki"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/recipe5"
        app:layout_constraintTop_toBottomOf="@+id/recipe4"
        tools:srcCompat="@tools:sample/backgrounds/scenic" />

    <Button
        android:id="@+id/toMain1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginLeft="37dp"
        android:layout_marginEnd="227dp"
        android:layout_marginRight="227dp"
        android:layout_marginBottom="30dp"
        android:fontFamily="serif"
        android:text="戻る"
        android:textColor="#7A6161"
        app:backgroundTint="#EFEAE2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/category1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="60dp"
        android:layout_marginBottom="570dp"
        android:text="和食"
        android:textColor="#7A6161"
        app:backgroundTint="#EFEAE2"
        app:layout_constraintBottom_toTopOf="@+id/toMain1"
        app:layout_constraintEnd_toStartOf="@+id/category2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/category2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="60dp"
        android:layout_marginBottom="570dp"
        android:text="洋食"
        android:textColor="#7A6161"
        app:backgroundTint="#EFEAE2"
        app:layout_constraintBottom_toTopOf="@+id/toMain1"
        app:layout_constraintEnd_toStartOf="@+id/category3"
        app:layout_constraintStart_toEndOf="@+id/category1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.49" />

    <Button
        android:id="@+id/category3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="570dp"
        android:text="中華"
        android:textColor="#7A6161"
        app:backgroundTint="#EFEAE2"
        app:layout_constraintBottom_toTopOf="@+id/toMain1"
        app:layout_constraintEnd_toStartOf="@+id/category4"
        app:layout_constraintStart_toEndOf="@+id/category2"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/category4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="570dp"
        android:text="デザート"
        android:textColor="#7A6161"
        app:backgroundTint="#EFEAE2"
        app:layout_constraintBottom_toTopOf="@+id/toMain1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/category3"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>